#include <stdio.h>

int main(void)
{
    int a;
    int b = 0,c = 0;
    for(int i = 0; i < 10; i++)
    {
        scanf("%d",&a);
        if(a % 5 == 0)
        {
            b++;
        }
        if(a % 7 == 0)
        {
            c++;
        }
    }
    printf("5의 배수 : %d\n",b);
    printf("7의 배수 : %d\n",c);
    return 0;
}
