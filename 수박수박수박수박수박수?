#include <stdio.h>

int main(void)
{
    int a;
    scanf("%d",&a);
    if(a % 2 == 0)
    {
        for(int i = 0; i < a/2; i++)
        {
            printf("수박");
        }
    }
    else if(a == 1)
    {
        printf("수");
    }
    else
    {
        for(int i = 0; i < a/2; i++)
        {
            if(a % 2 == 1)
            {
                if(1)
                {
                    printf("수");
                }
                if(1)
                {
                    printf("박");
                }
            }
        }
        printf("수");
    }
    return 0;
}
