#include <stdio.h>

int main(void)
{
    int a,b;
    char c;
    scanf("%d %c %d",&a,&c,&b);
    
    switch(c)
    {
        case '+':
            printf("결과 : %d",a + b);
            break;
        case '*':
            printf("결과 : %d",a * b);
            break;
        case '/':
            printf("결과 : %d",a / b);
            break;
        case '%':
            printf("결과 : %d",a % b);
            break;
        case '-':
            printf("결과 : %d",a - b);
        default:
            printf("ERROR : 산술연산자가 아닙니다.");
    }
    return 0;
}
