#include <stdio.h>

int MAX(int a, int b)
{
    if (a > b) {
        return a;
    }
    else
    {
        return b;
    }
}
int MIN(int a, int b)
{
    if (a > b) {
        return b;
    }
    else
    {
        return a;
    }
}

int main(void)
{
    int n1,n2;
    int max = 0;
    int min = 0;
    scanf("%d %d",&n1,&n2);
    max = MAX(n1, n2);
    min = MIN(n1, n2);
    printf("%d %d",min * 2, max / 2);
    return 0;
}
