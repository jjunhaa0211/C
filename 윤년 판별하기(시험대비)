#include <stdio.h>

int main(void)
{
    int a;
    scanf("%d",&a);
    if(a % 4 == 0 && a % 100 != 0)
    {
        printf("윤년입니다.");
    }
    else if(a % 400 == 0 && a % 100 ==0)
    {
        printf("윤년입니다.");
    }
    else if(a % 400 == 0)
    {
        printf("윤년입니다.");
    }
    else
    {
        printf("윤년이 아닙니다.");
    }
    return 0;
}
