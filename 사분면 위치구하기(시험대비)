#include <stdio.h>

int main(void)
{
    int n1,n2;
    scanf("%d %d",&n1,&n2);
    if(n1 > 0 && n2 > 0)
    {
        printf("1사분면");
    }
    else if(n1 > 0 && n2 < 0)
    {
        printf("4사분면");
    }
    else if(n1 < 0 && n2 > 0)
    {
        printf("2사분면");
    }
    else
    {
        printf("3사분면");
    }
    return 0;
}
