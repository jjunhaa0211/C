#include <stdio.h>

int main(void)
{
    int a,b;
    printf("주사위 두 개의 숫자를 입력하세요.(1~6) : ");
    scanf("%d %d",&a,&b);
    if(a >= 4 && b >= 4)
    {
        printf("이겼습니다.");
    }
    else if((a >= 4 && b < 4) || (a < 4 && b >= 4))
    {
        printf("비겼습니다.");
    }
    else
    {
        printf("졌습니다.");
    }
    return 0;
}
