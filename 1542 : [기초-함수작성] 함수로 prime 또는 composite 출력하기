#include <stdio.h>

int n;

void f()
{
    for(int i = 2; ; i++)
    {
        if(n / i == 1)
        {
            printf("prime");
            break;
        }
        else if(n % i == 0)
        {
            printf("composite");
            break;
        }
    }
}
int main()
{
  scanf("%d", &n);
  f(n);
  return 0;
}
