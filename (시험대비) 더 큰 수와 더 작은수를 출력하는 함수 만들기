
#include <stdio.h>

double MIN(double a,double b)
{
    if(a > b)
    {
        return b;
    }
    else
    {
        return a;
    }
}

double MAX(double c,double d)
{
    if (c > d)
    {
        return c;
    }
    else
    {
        return d;
    }
}

int main(void)
{
    double a,b;
    double c,d;
    scanf("%lf %lf",&a,&b);
    printf("%.2lf",MIN(a, b));
    scanf("%lf %lf",&c,&d);
    printf("%.2lf",MAX(c, d));
    return 0;
}
