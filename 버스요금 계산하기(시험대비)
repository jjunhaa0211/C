#include <stdio.h>

int main(void)
{
    int a;
    scanf("%d",&a);
    
    if(1 <= a && a <= 4)
    {
        printf("최종요금:무료\n");
    }
    if(4 <= a && a <= 13)
    {
        printf("최종요금:%d\n", 1000 / 2);
    }
    else if(14 <= a && a <= 19)
    {
        printf("최종요금:%d\n", 1000 / 4);
    }
    else if(20 <= a && a <= 64)
    {
        printf("최종요금:%d\n",1000);
    }
    else if(65 <= a)
    {
        printf("최종요금:무료\n");
    }
    return 0;
}
