#include <stdio.h>

int main(void)
{
    int arr[3][3] = { 0 };
    int sum = 0;
    for(int i = 0; i < 3; i++)
    {
        for(int j = 0; j < 3; j++)
        {
            scanf("%d",&arr[i][j]);
            sum = arr[i][j] || 1;
            printf("%d\t\t",sum);
        }
        printf("\n");
    }
    for(int i=0;i<3;i++){
        for(int j=0;j<3;j++) printf("%d ",arr[i][j]);
        printf("\n");
    }
}
