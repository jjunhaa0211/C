#include <stdio.h>

int main(void)
{
    int arr1[3][4] = { 0 };
    int arr2[3][4] = { 0 };
    int sum = 0;
    
    printf("첫번째 배열\n");
    for(int i = 0; i < 3; i++)
    {
        for(int j = 0; j < 4; j++)
        {
            scanf("%d",&arr1[i][j]);
        }
    }
    
    printf("두번째 배열\n");
    for(int i = 0; i < 3; i++)
    {
        for(int j = 0; j < 4; j++)
        {
            scanf("%d",&arr2[i][j]);
        }
    }
    
    for(int i = 0; i < 3; i++)
    {
        for(int j = 0; j < 4; j++)
        {
            sum = (arr1[i][j] * arr2[i][j]);
            printf("%d ",sum );
        }
        printf("\n");
        
    }
    return 0;
}
